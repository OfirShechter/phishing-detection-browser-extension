{
    "compilerOptions": {
      "target": "es6",
      "resolveJsonModule": true,
      "module": "Node16",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "sourceMap": true,
      "noImplicitAny": false,
      "moduleResolution": "node16"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "**/*.spec.ts"]

  }