{
  "name": "phishing-extension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && pnpm run build:main && pnpm run build:content && pnpm run build:background",
    "build:main": "vite build --config vite.main.config.ts",
    "build:content": "vite build --config vite.content.config.ts",
    "build:background": "vite build --config vite.background.config.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:extension": "tsx tests/test-extension.ts",
    "check-phishing:build": "vite build --config vite.script.config.ts",
    "check-phishing:run": "node dist-tests/openPhish.test.js",
    "check-phishing": "pnpm run check-phishing:build && pnpm run check-phishing:run"
  },
  "dependencies": {
    "@types/jsdom": "^21.1.7",
    "csv-parse": "^5.6.0",
    "gpt-tokenizer": "^2.9.0",
    "ml-confusion-matrix": "^2.0.0",
    "nodeplotlib": "^1.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^6.1.17",
    "tldts": "^7.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/chrome": "^0.0.313",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "node-fetch": "^3.3.2",
    "playwright": "^1.52.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-static-copy": "^2.3.0"
  }
}
